<UserControl x:Class="MsBuildTaskExplorer.SettingsWindowControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MsBuildTaskExplorer"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <StackPanel Margin="12,12,12,0">
        <Button Height="22" Content="Back" Click="BackButtonOnClick"/>
        <HeaderedContentControl Header="Supported File Extensions:" Margin="0,12,0,0">
            <TextBox Height="22" x:Name="SupportedExtTb" Margin="0,2,0,0" Padding="0,2,0,0" Background="Transparent" />
        </HeaderedContentControl>
    </StackPanel>
</UserControl>
